    <inlineHelpText>Logs will only be generated with a severity that is equal to or greater than the minimum selected level. Order from highest to lowest: ERROR, WARN, INFO, DEBUG.</inlineHelpText>
    <label>Minimum Severity</label>
    <required>false</required>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>ERROR</fullName>
                <default>true</default>
                <label>ERROR</label>
            </value>
            <value>
                <fullName>WARN</fullName>
                <default>false</default>
                <label>WARN</label>
            </value>
            <value>
                <fullName>INFO</fullName>
                <default>false</default>
                <label>INFO</label>
            </value>
            <value>
                <fullName>DEBUG</fullName>
                <default>false</default>
                <label>DEBUG</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
